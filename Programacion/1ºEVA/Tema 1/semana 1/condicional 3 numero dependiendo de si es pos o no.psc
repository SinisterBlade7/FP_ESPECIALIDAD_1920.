Algoritmo sin_titulo
	definir a como entero;
	definir b como entero;
	definir c como entero;
	escribir"a-->";leer a;
	escribir"b-->";leer b;
	escribir"c-->";leer c;
	
	si a<0 entonces
		
		escribir"El resultado es-->", a*b*c;
	FinSi
	si a>=0 entonces
		
		escribir"El resultado es-->", a+b+c;
		
	FinSi
	
	
	
FinAlgoritmo
