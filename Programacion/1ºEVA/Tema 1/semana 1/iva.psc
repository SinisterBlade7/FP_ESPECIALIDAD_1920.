Algoritmo sin_titulo
	Definir precio Como Entero;
	Definir iva Como Entero;
	Definir total Como Real;
	Escribir "dime el precio del producto "; Leer precio;
	Escribir "dime el porcentaje del iva"; Leer iva;
	total=(precio*(100+iva))/100;
	Escribir " en total cuesta"; Escribir total," euros";
	
	
FinAlgoritmo
