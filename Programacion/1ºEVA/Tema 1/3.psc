Algoritmo sin_titulo
	definir rnum como caracter;
	Escribir "Asegurese de insertar el caracter en mayúsculas";
	leer rnum;
	Segun rnum Hacer
		"I":
			escribir "1";
		"V":
			escribir "5";
		"X":
			escribir "10";
		"L":
			escribir "50";
		"C":
			escribir "100"
		"D":
			escribir "500"
		"M":
			escribir "1000"
		De Otro Modo:
			escribir "error";
	Fin Segun
	
	
FinAlgoritmo
